  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "PORT=3000 nest start --watch",
    "start:debug": "PORT=3000 nest start --debug --watch",
    "start:prod": "PORT=3000 node dist/main",
    "worker:dev": "ts-node -r tsconfig-paths/register src/worker.main.ts",
    "worker:prod": "node dist/worker.main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node -r dotenv/config node_modules/.bin/jest --config ./test/jest-e2e.json",
    "cli": "ts-node -r tsconfig-paths/register src/cli/cli.ts",
    "create-admin": "npm run cli create-admin",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:run": "npm run typeorm migration:run -- -d src/database/config/database-source.ts",
    "migration:generate": "npm run typeorm -- -d src/database/config/database-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:revert": "npm run typeorm -- -d src/database/config/database-source.ts migration:revert"
  },
