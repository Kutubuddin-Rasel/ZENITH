{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false
  },
  "projects": {
    "main": {
      "type": "application",
      "root": ".",
      "entryFile": "main",
      "sourceRoot": "src"
    },
    "worker": {
      "type": "application",
      "root": ".",
      "entryFile": "worker.main",
      "sourceRoot": "src"
    }
  }
}